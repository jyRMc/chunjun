{
  "job": {
    "content": [
      {
        "reader": {
          "name": "binlogreader",
          "parameter": {
            "schema": "flink_realtime",
            "table": ["flink_realtime.source"],
            "database": "flink_realtime",
            "username": "root",
            "password": "admin123456",
            "cat" : "insert,delete,update",
            "jdbcUrl": "jdbc:mysql://localhost:3306/flink_realtime?useUnicode=true&characterEncoding=UTF-8&autoReconnect=true&useSSL=false&zeroDateTimeBehavior=convertToNull&serverTimezone=Asia/Shanghai",
